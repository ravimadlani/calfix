# Application Port
VITE_PORT=3001

# Google OAuth Configuration
# Note: Client secrets are NO LONGER used client-side. OAuth tokens are fetched
# from Clerk via /api/calendar/token endpoint for security.
VITE_GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com
VITE_REDIRECT_URI=http://localhost:3001

# Microsoft Outlook OAuth Configuration
# Note: Client secrets are NO LONGER used client-side. OAuth tokens are fetched
# from Clerk via /api/calendar/token endpoint for security.
VITE_OUTLOOK_CLIENT_ID=your-outlook-client-id
VITE_OUTLOOK_REDIRECT_URI=http://localhost:3001
VITE_OUTLOOK_TENANT_ID=common

# Clerk Authentication
VITE_CLERK_PUBLISHABLE_KEY=pk_test_xxxxx
CLERK_SECRET_KEY=sk_test_xxxxx
CLERK_WEBHOOK_SECRET=whsec_xxxxx

# Supabase Database
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
